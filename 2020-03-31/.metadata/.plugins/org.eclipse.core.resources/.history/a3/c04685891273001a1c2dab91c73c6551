package Ex03;

public class Sample03 {

	public static void main(String[] args) {
		
		System.out.println("학번\t이름\t국어점수\t영어점수\t수학점수\t총점\t평균\t\t\t\t학점");
		Grade03 g1=new Grade03("01","홍길동",90,80,100);
		g1.printGrade(g1);
		
		Grade03 g2=new Grade03("02","심청이",93,87,43);
		g2.printGrade(g2);
		
	}

}
