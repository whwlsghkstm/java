package Ex01;

public class Student {
	
	//필드
	String sno;
	String sname;
	String sph;
	
	//기본생성자 - 생략가능
	Student() {
		
	}
	
	//method - 동작
	public void printStudent(Student s) {
		System.out.println("학번\t이름\t연락처");
		System.out.println(s.sno + "\t" + s.sname + "\t" + sph + "km/h");
		System.out.println();
	}

}
