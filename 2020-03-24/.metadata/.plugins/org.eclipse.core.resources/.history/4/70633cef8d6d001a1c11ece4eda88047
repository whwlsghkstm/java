package Ex05;

public class Sample {

	public static void main(String[] args) {
		
		switch1();

	}
	
	public static void switch1() {
		System.out.println("switch 예제1");
		System.out.println("------------------------------------");
		int time = 2;
		
		switch(time) {
		case 9:
			System.out.println("회의를 시작합니다.");
			break;
		case 8:
			System.out.println("출근시간 입니다.");
			break;
		case 10:
			System.out.println("업무를 시작합니다.");
			break;
		default :
			System.out.println("외근을 합니다.");
			break;
		}
	}

}
